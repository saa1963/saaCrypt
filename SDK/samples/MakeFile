DIRS = \
    CSP\~ \
    SSPI\~ \

SDKPROJ =samples

!include "dirprop.mak"

all: $(DIRS)

csources: mkd $(DIRS)
    @copy makefile "$(dest)"
    @copy dir.mak "$(dest)"
    @copy dirprop.Mak "$(dest)"
    @copy samples.mak "$(dest)"
    @copy readme.txt "$(dest)"

mkd:
    -mkdir "$(dest)"

clean:
    @$(MAKE) -nologo /$(MAKEFLAGS) clean=1

$(DIRS):
!include "dir.mak"
