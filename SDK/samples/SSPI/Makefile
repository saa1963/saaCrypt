DIRS = \
    WebClient\~ \
    WebServer\~ \
       	
SDKPROJ =SSPI

!include "..\dirprop.mak"

all: $(DIRS)

clean:
    @$(MAKE) -nologo /$(MAKEFLAGS) clean=1

csources: mkd $(DIRS)
    @copy makefile "$(dest)"
    @copy sspi.mak "$(dest)"
    @copy ReadMe.Txt "$(dest)"

mkd:
    -mkdir "$(dest)"

$(DIRS):
!include "..\dir.mak"
